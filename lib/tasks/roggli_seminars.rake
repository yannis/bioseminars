namespace :roggli do
  desc "Enter roggli's seminars"
  task :seminars => :environment do
    new_seminars = []
    for s in SEMINARS
      sem = Seminar.new
      sem.start_on = s[1]
      sem.end_on = Time.parse(s[1])+s[2].minutes
      sem.location = Location.find_or_initialize_by_name(s[3])
      sem.location.building = Building.find_by_name('Sciences III') if sem.location.building.blank?
      if s[4] = 'lsss'
        sem.category = Category.find_by_name('Life Science Seminar Series')
      elsif s[4] = 'ord'
        sem.category = Category.find_by_name('Dpt of Molecular Biology')
      end
      for h in s[-1]
        h_from_hosts = HOSTS.detect{|i| i[0] == h}
        unless h_from_hosts.nil?
          host = Host.find_by_email(h_from_hosts[-1].downcase)
          if host.nil?
            sem.hosts.build(:email => h_from_hosts[-1].downcase, :name => h_from_hosts[2]+' '+h_from_hosts[3].capitalize)
          else
            sem.hosts << host
          end
        end
      end
      speaker_from_list = SPEAKERS.detect{|i| i[0] == s[0]}
      # puts speaker_from_list
      sem.speakers.build(:name => speaker_from_list[2]+' '+speaker_from_list[3], :affiliation => speaker_from_list[4], :title => speaker_from_list[5]) unless speaker_from_list.nil?
      sem.user = User.find_by_email('yannis.jaquet@unige.ch'.downcase)
      new_seminars << sem # unless sem.speakers.blank? or sem.hosts.blank?
    end

    new_seminars.each do |s|
      puts "hosts: #{s.hosts.size}; speakers: #{s.speakers.size}"
    end
  end
end