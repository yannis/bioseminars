  //    border: none
  //  span.current
  //    color: $veryverylightgreen
  //    background-color: $verylightred
  //  a
  //    -moz-border-radius: .5em
  //    -webkit-border-radius: .5em
  //    -border-radius: .5em
  //    color: #FFF
  //    text-decoration: none
  //    &:hover, &:focus
  //      border-color: $verydarkgreen
  //      background: $darkgreen
  //      color: $veryverylightgreen
  //  .page_info
  //    color: #aaa
  //    padding-top: .8em
  //  .prev_page, .next_page
  //    border-width: 2px
  //  .prev_page
  //    margin-right: .5em
  //  .next_page
  //    margin-left: .5em
  //<- Pagination
  .pagination
    font-size: 1.2em
    font-weight: bold
    a, em, span
      background: $green
      border: 1px solid $green
      -moz-border-radius: .3em
      -webkit-border-radius: .3em
      border-radius: .3em
      color: white
      margin: 0
      padding: .3em
    span.disabled, span.gap
      background: transparent
      border: 0
      color: $verylightgreen
      -webkit-box-shadow: none
      -moz-box-shadow: none
      box-shadow: none
    a
      background: transparent
      border: 1px solid $red
      color: $red
      text-decoration none
      &:hover, &:focus
        background-color: $lightred
        border-color: $lightred
        color: $white
  // -> #about
  #about
    p
      font-size: 118%
      margin-bottom: 1em
  //<- #about
  //-> .scope_and_pagination
  .scope_and_pagination
    width: 100%
    #scope
      display: inline-block
      font-size: 118%
      select
        font-size: 100%
    .pagination
      float: right
      margin: 1.2em 0 0 0
  //<- scope_and_pagination
.alert_internal
  color: $red
  text-align: center
  font-size: 145%
//<- end Content

.box
  background-color: white
  border-radius: .5em
  -moz-border-radius: .5em
  -webkit-border-radius: .5em
  margin-bottom: 1em
  padding: 1em
  h1, h2, h3, h4
    color: $lightred
  h1
    padding-bottom: .6em
    border-bottom: 2px solid $lightred
    font:
      size: 2em // 24/30=0.8
  h2
    font-size: 1.5em
    line-height: 1.2em
    margin: 1em 0


  // -> Footer
#footer
  margin: 2em 0 1em 0
  width: 100%
  #logo
    float: left
    width: 25%
    #logo_fac
      width: 100%
  #footer_box
    float: right
    width: 73%
    position: relative
    display: block
    text-align: center
    min-height: 100%
    overflow: hidden
    color: #FFF
    background-color: $green
    -moz-border-radius: .5em
    -webkit-border-radius: .5em
    -border-radius: .5em
    #rss_feed_link, #ical_feed_link
      -moz-background-clip: border
      -moz-background-inline-policy: continuous
      -moz-background-origin: padding
      padding: 1px 0 5px 19px
    #rss_feed_link
      background: transparent url('/images/rssIconS.png') no-repeat scroll left top
    #ical_feed_link
      background: transparent url('/images/ical_iconS.png') no-repeat scroll left top
    a
      color: #FFF
      font-weight: bold
      text-decoration: none
    ul
      width: 40%
      float: left
      position: relative
      list-style: none
      text-align: left
      margin: 1em 5%
      li
        width: 100%
        font-size: 118%
        padding: .5em 0
        border-bottom: 1px solid
        border-bottom-color: $lightgreen
        margin: 0
        &:last-child
          border: none
// <- End Footer
//-> Various
span.by
  font-family: Baskerville,"Goudy Old Style","Palatino","Book Antiqua",serif
  font-size: 1.2em
  font-style: italic
  font-weight: normal
  line-height: 1




.gritter-item-wrapper.notice
  background-color: rgba(80,146,9,0.8)
.gritter-item-wrapper.warning
  background-color: rgba(250,21,18,0.8)

#ZoomImage
  border: 1em solid white
  -webkit-box-shadow: 2px 2px 5px rgba(0,0,0,.5)
  -moz-box-shadow: 2px 2px 5px rgba(0,0,0,.5)
  box-shadow: 2px 2px 5px rgba(0,0,0,.5)
