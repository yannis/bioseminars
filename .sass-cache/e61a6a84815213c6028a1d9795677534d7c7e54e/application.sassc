#javascript_disabled_notification
  font-size: 118%
  padding: 1em
  border: 2px solid
  border-color: $red
  background-color: $veryverylightred
  width: 60%
  margin: 1em auto
  -moz-border-radius: .5em
  -webkit-border-radius: .5em
  -border-radius: .5em
  h2
    line-height: 1em
    margin-bottom: 1em
  a:hover
    color: #FFF
  p
    margin-bottom: 1em
#errors
  -moz-border-radius: .5em
  -webkit-border-radius: .5em
  -border-radius: .5em
  padding: 1em
  margin: auto
  width: 50%
  border: 2px solid
  border-color: $red
  h2, h3
    line-height: 1em
    margin-bottom: 1em
  p
    line-height: 1em
    margin-bottom: 1em
    font-size: 118%
/* css for timepicker */
.ui-datepicker
  display: none
.ui-timepicker-div
  font:
    size: 0.9em
    weight: bold
  .ui-widget-header
    margin-bottom: 8px
  dl
    text-align: left
    dt
      height: 25px
    dd
      margin: -25px 0 10px 65px
  .ui_tpicker_hour div
    padding-right: 2px
  .ui_tpicker_minute div
    padding-right: 6px
  .ui_tpicker_second div
    padding-right: 6px
  td
    font-size: 90%
  .ui-datepicker-close.ui-state-default.ui-priority-primary.ui-corner-all
    background-color: red
