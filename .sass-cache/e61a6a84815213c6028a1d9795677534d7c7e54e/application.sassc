/* HTML5 tags */
header, section, footer, aside, nav, article, figure
  display: block
body
  background-color: $background
  color: #333
  font-family: "Lucida Grande", helvetica, arial, sans-serif
  font-size:   12px
  line-height: 1.5em
h1
  font-size: 250%
  line-height: 1em
h2
  font-size: 218%
  line-height: 1em
  &.title
    margin-bottom: 1em
h3
  font-size: 164%
  line-height: 1em
h4
  font-size: 145%
  line-height: 1em
h5
  font-size: 118%
  line-height: 1em
a, a:visited
  text-decoration: none
  color: $darkred
  &:hover
    color: $veryverylightred
  -webkit-transition-property: color
  -webkit-transition-duration: .2s
  -webkit-transition-timing-function: linear
  img
    text-decoration: none
  &[href^="http:"]
    background-repeat: no-repeat
    background-position: right center
    background-image: url(icons/external.png)
    margin: -2px 0
    padding: 2px 14px 2px 0
  &[href^="mailto:"]
    background-repeat: no-repeat
    background-position: right center
    background-image: url('/images/icons/email.png')
    padding: 2px 18px 2px 0
  &[href*=".pdf"]
    background-repeat: no-repeat
    background-position: right center
    background-image: url('/images/icons/pdf.png')
    padding: 2px 18px 2px 0
  &[href*=".doc"]
    background-repeat: no-repeat
    background-position: left center
    background-image: url(icons/doc.png)
    margin: -2px 0
    padding: 2px 20px 2px 18px
  &[href*=".xls"]
    background-repeat: no-repeat
    background-position: left center
    background-image: url('/images/icons/xls.png')
    margin: -2px 0
    padding: 2px 0 2px 18px
  &[href*=".rss"]
    background-repeat: no-repeat
    background-position: right center
    background-image: url('/images/icons/feed.png')
    margin: -2px 0
    padding: 2px 22px 2px 0
  &[href*=".rdf"]
    background-repeat: no-repeat
    background-position: right center
    background-image: url('/images/icons/feed.png')
    margin: -2px 0
    padding: 2px 22px 2px 0
  &[href*=".xls"]
    background-repeat: no-repeat
    background-position: right center
    background-image: url('/images/icons/xls.png')
    margin: -2px 0
    padding: 2px 22px 2px 0
  &[href*=".ics"]
    background-repeat: no-repeat
    background-position: right center
    background-image: url('/images/icons/ics.png')
    margin: -2px 0
    padding: 2px 18px 2px 0
// -> Main Container
#main_container
  margin: 0 auto
  //960px/12px = 80 (12px base font-size)
  //max-width: 80em
  width: 92%

  // -> Banner and navigation
  #banner_and_navigation
    width: 100%
    margin: 0 0 1em 0
    // -> Banner
    #banner
      float: left
      width: 25%
      overflow: visible
      h1
        font-size: 436%
        margin:  .1em 0 .2em 0
        .banner_font
        a
          z-index: 500
          text-decoration: none
          color: $green
          &:hover
            color: $verylightred
    // <- end banner
    // -> Navigation
    ul#navigation
      float: right
      text-align: center
      font-size: 145%
      padding: 0
      height: 4em
      position: relative
      top: -1em
      li
        float: left
        height: 2.5em
        margin: 0 .5em
        background-color: $verylightgreen
        position: relative
        z-index: 100
        a
          display: block
          width: 7em
          color: $white
          -moz-border-radius: .5em
          -webkit-border-radius: .5em
          -border-radius: .5em
          -webkit-box-shadow: 2px 2px 5px rgba(0,0,0,.5)
          -moz-box-shadow: 2px 2px 5px rgba(0,0,0,.5)
          box-shadow: 2px 2px 5px rgba(0,0,0,.5)
          -webkit-transition-property: background-color
          -webkit-transition-duration: .4s
          -webkit-transition-timing-function: linear
          padding: 1.4em 0em .5em 0em
          margin: 0
          background-color: $verylightgreen
          &:hover
            background-color: $veryverylightred
          &:active
            background-color: $verylightred
        &.selected
          a
            background-color: $green
            padding: 2em 0em .5em 0em
            &:hover
              background-color: $lightred
          &:hover
            ul
              padding: 3.6em 0 .5em 0
              li a:hover
                background-color: $lightred
        ul
          height: auto
          background-position: 0 0 !important
          display: none
          width: 6em
        &:hover
          ul
            display: block
            z-index: -100
            position: absolute
            top: 0
            padding: 3.1em 0 .5em 0
            margin: 0 .5em
            -moz-border-radius: .5em
            -webkit-border-radius: .5em
            -border-radius: .5em
            -webkit-box-shadow: 2px 2px 5px rgba(0,0,0,.5)
            -moz-box-shadow: 2px 2px 5px rgba(0,0,0,.5)
            box-shadow: 2px 2px 5px rgba(0,0,0,.5)
            background-color: $verylightgreen
            li
              float: none
              z-index: 20
              margin: 0
              height: 1.6em
              a
                padding: .3em 0
                margin: 0
                width: 6em
                background-color: $verylightgreen
                -webkit-box-shadow: none
                -moz-box-shadow: none
                box-shadow: none
                -moz-border-radius: 0
                -webkit-border-radius: 0
                -border-radius: 0
                &:hover
                  background-color: $veryverylightred
              //ul
              //  position: relative
              //  top: -1.6em
              //  left: 5.5em
              //  display: none
              //  width: 5em
              //&:hover
              //  ul
              //    display: block
              //    z-index: 10
              //    padding: 0
              //    background-color: $veryverylightred
              //    li
              //      background-color: transparent
              //      a
              //        -moz-border-radius: 0 0 .5em .5em
              //        -webkit-border-radius: 0 0 .5em .5em
              //        -border-radius: 0 0 .5em .5em
              //        width: 5em
              //        background-color: transparent

    // <- end Navigation
  // <- end Banner and Navigation
// <- end Main Container
// -> Flash and Co
#flash_and_co
  width: 100%
  &>div
    .flash_close
      display: none
      float: right
      position: relative
      top: -1em
      left: 1.2em
      cursor: pointer
    &:hover
      .flash_close
        display: block
  .flash
    margin-bottom: 1em
  .notice, .alert
    width: 100%
    margin: 0 auto 1em auto
    padding: 1em 0
    text-align: center
    font-size: 1.5em
    font-weight: 1.5em
    -moz-border-radius: .5em
    -webkit-border-radius: .5em
    -border-radius: .5em
    border: 2px solid
  .notice
    background-color: $veryverylightgreen
    border-color: $green
    color: $verydarkgreen
  .alert
    background-color: $veryverylightred
    border-color: $red
    color: $verydarkred
// <- end Flash and Co
#content
  min-height: 35em
  margin-bottom: 1em
  table
    &.list
      margin: 0 0 1em 0
      padding: 0
      width: 100%
      border-collapse: collapse
      tr
        th:first-child, td:first-child
          -moz-border-radius: .5em 0 0 .5em
          -webkit-border-bottom-left-radius: .5em
          -webkit-border-top-left-radius: .5em
          border-radius: .5em 0 0 .5em
        th:last-child, td:last-child
          -moz-border-radius: 0 .5em .5em 0
          -webkit-border-bottom-right-radius: .5em
          -webkit-border-top-right-radius: .5em
          border-radius: 0 .5em .5em 0
      thead
        th
          color: $veryverylightgreen
          background-color: $green
          vertical-align: middle
      tbody
        color: $verydarkgreen
        tr:nth-child(even), .odd
          background-color: $veryverylightgreen
        tr.dragged
          background-color: yellow
      td, th
        padding: .2em .5em
        text-align: left
    &.calendar
      .seminar
        border: 0 none
  .color
    display: inline-block
    margin: 0 .1em
    vertical-align: middle
    width: 1em
    height: 1em
  .admin_links
    text-align: right

  #seminars
    margin-top: 1em
    width: 61%
    h2
      font-size: 145%

  //-> seminar unit
  .seminar
    border-top: 2px solid
    border-color: $verylightgreen
    .time_and_location
      position: relative
      border-bottom: 1px dotted
      border-color: $verylightgreen
      padding: .5em 0
      color: #333
      font-weight: bold
    .title_and_content
      h2, h3, p
        line-height: 1.5em
        margin: .6em 0
      ul li
        margin: .6em 0
      p.creator
        text-align: right
    &.hidden_seminar
      display: none
    ul#pictures
      list-style: none
      margin: 0
      li
        float: left
        margin-right: 1em
        & a img
          +image_thumbnail
    .seminar_publications
      dl
        dt
          font-size: 118%
        dd
          p
            margin-bottom: 0
          p:last-child
            margin-bottom: 1em
      p#big_ajax_loader
        text-align: center
        font-weight: bold
        img
          display: block
          position: relative
          margin: 1em auto 0 auto
          background-color: $green
          padding: 1em
          -moz-border-radius: 1em
          -webkit-border-radius: 1em
          -border-radius: 1em
  //<- seminar unit
  //-> left column
  .left_column
    float: left
    width: 73%
    h2.title
      margin-bottom: 1em
    h3, h4
      line-height: 1.5em
      margin: 1em 0 .5em 0
    dl
      dt
        font-size: 118%
        font-weight: bold
      dd
        margin-bottom: 1em
    #links_to_next_and_previous
      margin-bottom: 1em
      text-align: right
      a
        padding: .5em
        -moz-border-radius: .5em
        -webkit-border-radius: .5em
        -border-radius: .5em
        background-color: $verylightred
        color: #FFF
        font-weight: bold
        &:hover
          background-color: $veryverylightred

  //<- left column
  //-> right column
  aside
    float: right
    width: 25%
    section#categories
      background-color: #FFF
      -moz-border-radius: .5em
      -webkit-border-radius: .5em
      -border-radius: .5em
      padding: 1em
      z-index: 50
      form
        padding: 0
      h3
        color: $lightred
        margin: 0
        padding-bottom: 1em
      ul
        margin: 1em 0
        li
          display: block
          padding: .3em 0
          margin: 0
          overflow: hidden
          border-bottom: 1px solid $veryverylightred
          label, span.label
            float: left
            width: 80%
          input
            &[type=checkbox]
            float: right
            text-align: right
            width: 19%
          &:last-child
            border: none
          .label
            font-weight: normal
      &.categories_hidden
        margin-bottom: 1em
        .title
          a
            float: right
        #ZoomClose
          display: none
        .hidden_categories
          display: none

      &.all_categories_shown
        width: 100%
        position: absolute
        left: 8%
        width: 80%
        background-color: #FFF
        padding: 1em 1em 1em 1em
        -moz-border-radius: .5em
        -webkit-border-radius: .5em
        -border-radius: .5em
        -webkit-box-shadow: 2px 2px 5px rgba(0,0,0,.5)
        -moz-box-shadow: 2px 2px 5px rgba(0,0,0,.5)
        box-shadow: 2px 2px 5px rgba(0,0,0,.5)
        border: 2px solid
        border-color: $green
        img#ZoomClose
          display: block
          position: absolute
          left: -15px
          top: -15px
          cursor: pointer
          z-index: 20
        #all_categories
          width: 100%
          border-top: 2px solid
          border-top-color: $background
          ul
            float: left
            width: 24%
            margin: 1em 1% 1em 0
            border: none
            &:last-child
              margin-right: 0
      .admin_links
        a#create_feeds_link
          margin-top: 1em
          padding: 1px 19px 0 0
          background: transparent url('/images/rssIconS.png') no-repeat scroll right top
  //<- right column
  //-> Pagination
  //.pagination
  //  font-size: 118%
  //  margin: 1em 0
  //  a, span
  //    margin: 0 .2em 0 0
  //    padding: .3em .5em
  //    background-color: $green
  //    -moz-border-radius: .5em
  //    -webkit-border-radius: .5em
  //    -border-radius: .5em
  //  span.disabled
